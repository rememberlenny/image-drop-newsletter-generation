<h1>Listing introexams</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Message</th>
      <th>Picture1</th>
      <th>Testimonial1</th>
      <th>Picture2</th>
      <th>Testimonial2</th>
      <th>Picture3</th>
      <th>Testimonial3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @introexams.each do |introexam| %>
      <tr>
        <td><%= introexam.title %></td>
        <td><%= introexam.message %></td>
        <td><%= introexam.picture1 %></td>
        <td><%= introexam.testimonial1 %></td>
        <td><%= introexam.picture2 %></td>
        <td><%= introexam.testimonial2 %></td>
        <td><%= introexam.picture3 %></td>
        <td><%= introexam.testimonial3 %></td>
        <td><%= link_to 'Show', introexam %></td>
        <td><%= link_to 'Edit', edit_introexam_path(introexam) %></td>
        <td><%= link_to 'Destroy', introexam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Introexam', new_introexam_path %>
